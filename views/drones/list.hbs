<h2>Available drones</h2>

{{#each result}}

    <h3>{{this.name}}</h3>
    <ul>
        <li>Propellers: {{this.propellers}}</li>
        <li>Speed: {{this.maxSpeed}}</li>
    </ul>
    <a href='drones/{{this._id}}/edit'> Edit</a>
    <form action="/drones/{{this._id}}/delete" method="POST">
        <input type="hidden">
        <button type="submit">Delete</button>
    </form>
{{/each}}